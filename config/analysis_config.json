{
  "HISTORY_MODE": true,

  "repos_urls_path": "config/urls.json",
  "dirs_list_path": "config/dirs.json",
  "dump_dir_path" : "repos/analysis_results",

  "threads_count": 5,
  "log_dump_threshold": 200,
  "summary_dump_threshold": 200,

  "gzip_files": true,
  "remove_files_after_gzip": false,
  "remove_repo_after_analysis": true,

  "commits_type": "merges",
  "min_commits_number": 0,
  "merges_part_in_history": 0.0,

  "task": "name",
  "granularity": "method",
  "method_uniqueness": ["full_name", "name", "file", "return_type", "args_types"],
  "parser": "gumtree",
  "languages": ["Java", "Python"],

  "hide_methods_names": true,
  "exclude_constructors": true,

  "min_body_lines_length": 3,
  "exclude_with_exact_name": ["exactName1", "exactName2"],
  "exclude_with_name_prefix": ["test"],

  "JAVA_exclude_with_annotations": ["@Override"],

  "max_paths": 1000,
  "max_path_width": 2,
  "max_path_length": 9,
  "exclude_nodes": [],
  "exclude_doc_node": false,
  "ast_dot_format": false,
  "code2sec_format_dump": true
}
